<mvc:View xmlns:html="http://www.w3.org/1999/xhtml" xmlns:mvc="sap.ui.core.mvc" xmlns:un="sap.ui.unified" xmlns="sap.m" controllerName="image.ui5.controller.View1" displayBlock="true">
	<App id="idAppControl">
		<pages>
			<Page title="{i18n>title}">
				<content>
				    <sap.ui.layout.form:SimpleForm xmlns:sap.ui.layout.form="sap.ui.layout.form" xmlns:sap.ui.core="sap.ui.core" editable="true" layout="ResponsiveGridLayout" id="form0">
				        <sap.ui.layout.form:content>
				            <sap.ui.core:Title text="Configuration" id="title0"/>
				            <Label text="Image" id="label0"/>
				            <un:FileUploader id="fileUploader"/>
				            <ComboBox selectedItem="Element sap.ui.core.ListItem#__item0" value="List Item 1" selectedKey="classify" selectedItemId="__item0" id="selectionBox">
				                <items>
				                    <sap.ui.core:ListItem text="Generic Classification" key="classify" id="item0"/>
				                    <sap.ui.core:ListItem text="Human Detection" key="human" id="item1"/>
				                    <sap.ui.core:ListItem text="Face Detection" key="face" id="item2"/></items>
				            </ComboBox></sap.ui.layout.form:content>
				    </sap.ui.layout.form:SimpleForm>
				    <Button xmlns:html="http://www.w3.org/1999/xhtml" xmlns:mvc="sap.ui.core.mvc" xmlns:un="sap.ui.unified" xmlns="sap.m" text="Process" width="100%" id="buttonProcess" press="buttonProcess"/>
				    <HBox width="100%" id="hbox0" backgroundDesign="Translucent" justifyContent="Center">
				        <items>
				            <Image src="https://ui5.sap.com/resources/sap/ui/documentation/sdk/images/logo_ui5.png" width="140px" height="140px" id="image0"/>
				            <Image src="https://ui5.sap.com/resources/sap/ui/documentation/sdk/images/logo_ui5.png" width="140px" height="140px" id="image1"/>
				        </items>
				    </HBox>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>