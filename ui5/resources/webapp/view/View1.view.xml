<mvc:View xmlns:html="http://www.w3.org/1999/xhtml" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="ui5.ui5.controller.View1"
	displayBlock="true">
	<App id="idAppControl">
		<pages>
			<Page title="{i18n>title}">
				<content>
					<Panel id="__panel0" expandable="true" expanded="true" headerText="Settings">
						<content>
							<sap.ui.layout.form:SimpleForm xmlns:sap.ui.layout.form="sap.ui.layout.form" xmlns:sap.ui.core="sap.ui.core" editable="true"
								layout="ResponsiveGridLayout" id="__form0" enctype="multipart/form-data">
								<sap.ui.layout.form:content>
									<sap.ui.core:Title text="Parameters" id="__title0"/>
									<sap.ui.unified:FileUploader xmlns:html="http://www.w3.org/1999/xhtml" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"
										xmlns:sap.ui.layout.form="sap.ui.layout.form" xmlns:sap.ui.core="sap.ui.core" xmlns:sap.ui.unified="sap.ui.unified" id="fileUploader"
										placeholder="Upload pdf" width="auto" name="filename" uploadStart="onUploadStart"/>
									<SelectList selectedItem="Element sap.ui.core.ListItem#__item0" selectedKey="option1" selectedItemId="__item0" id="optionList">
										<items>
											<sap.ui.core:ListItem text="Face Detection" key="option1" id="__item0"/>
											<sap.ui.core:ListItem text="Human Detection" key="option2" id="__item1"/>
											<sap.ui.core:ListItem text="Classification" key="option3" id="__item2"/></items>
									</SelectList>
									<Label text="Label 2" id="__label1"/></sap.ui.layout.form:content>
							</sap.ui.layout.form:SimpleForm>
							<Button text="Process" width="100px" id="__button1" press="onButton"/>
						</content>
					</Panel>
					<Panel xmlns:html="http://www.w3.org/1999/xhtml" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" id="__panel0_copy" expandable="true"
						expanded="true" accessibleRole="Region" headerText="Result">
						<content>
							<Image width="140px" height="140px" id="__image1"/>
						</content>
					</Panel>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>